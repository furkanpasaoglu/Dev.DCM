@page
@using Dev.DCM.Localization
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model Dev.DCM.Web.Pages.CustomerMovementCodes.CreateModal
@inject IStringLocalizer<DCMResource> L
@{
Layout = null;
}
<abp-dynamic-form abp-model="CreateUpdateCustomerMovementCode" asp-page="/CustomerMovementCodes/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewCustomerMovementCode"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>