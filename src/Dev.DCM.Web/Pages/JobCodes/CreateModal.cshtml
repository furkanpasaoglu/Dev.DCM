@page
@using Dev.DCM.Localization
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model Dev.DCM.Web.Pages.JobCodes.CreateModal
@inject IStringLocalizer<DCMResource> L
@{
    Layout = null;
}

<abp-dynamic-form abp-model="JobCode" asp-page="/JobCodes/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewJobCode"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content/>
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel | AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>